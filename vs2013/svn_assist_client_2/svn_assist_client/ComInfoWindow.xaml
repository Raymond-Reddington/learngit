<Window x:Class="svn_assist_client.ComInfoWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="命令执行结果" Height="581.375" Width="730" >
    <Grid>
        <Label Content="执行命令：" HorizontalAlignment="Left" Height="24" Margin="27,33,0,0" VerticalAlignment="Top" Width="73"/>
        <Label Content="执行时间：" HorizontalAlignment="Left" Height="25" Margin="27,72,0,0" VerticalAlignment="Top" Width="73"/>
        <Label Content="执行结果：" HorizontalAlignment="Left" Height="25" Margin="27,114,0,0" VerticalAlignment="Top" Width="73"/>
        <TextBox x:Name="commandTextBox" HorizontalAlignment="Left" Height="24" Margin="105,33,0,0" TextWrapping="Wrap" IsReadOnly="True" Text="" VerticalAlignment="Top" Width="576"/>
        <TextBox x:Name="timeTextBox" HorizontalAlignment="Left" Height="24" Margin="105,72,0,0" TextWrapping="Wrap" IsReadOnly="True" Text="" VerticalAlignment="Top" Width="576"/>
        <TextBox ScrollViewer.VerticalScrollBarVisibility="Auto" x:Name="resultTextBox" HorizontalAlignment="Left" Height="376" Margin="105,114,0,0" TextWrapping="Wrap" IsReadOnly="True" Text="" VerticalAlignment="Top" Width="576"/>
        <Button Content="&lt;返回" Background="White" Foreground="Blue" BorderThickness="0" HorizontalAlignment="Left" Height="33" Margin="27,0,0,0" VerticalAlignment="Top" Width="53" Click="OnBack"/>

    </Grid>
</Window>
